<h2>List file auto dump from system</h2>
<table class="table" id="example" style="width:100%">
	<thead>
		<tr>
			<th>File name</th>
			<th>Last modified</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @obj.map do |file| %>
		<tr>
			<td><%= file.key %></td>
			<td><%= file.last_modified  %></td>
			<td>  <%= link_to 'Download', admins_dowload_file_database_path(:key => file.key),method: :post, class: "btn btn-info" %></td>
		</tr>
		<% end %>
	</tbody>
</table>
<%= link_to "Getting a file dump database now",  admins_manages_get_file_database_path,method: :post ,class: "btn btn-primary" %>
